{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@core/*": ["./@core/*"],
      "@layouts/*": ["./@layouts/*"],
      "@menu/*": ["./@menu/*"],
      "@assets/*": ["./assets/*"],
      "@components/*": ["./components/*"],
      "@configs/*": ["./configs/*"],
      "@views/*": ["./views/*"],
      "@chats/*": ["./app/view/main/content/Chat/*"],
      "@content/*": ["./types/apps/content/*"],
      "@shared/*": ["./components/layout/shared/*"],
      "@main/*": ["./app/view/main/*"],
      "@ui/*": ["./ui/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
